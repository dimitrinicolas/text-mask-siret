{
  "name": "text-mask-siret",
  "version": "1.0.0",
  "description": "Text mask for french SIRET and SIREN",
  "main": "dist/text-mask-siret.cjs.js",
  "module": "dist/text-mask-siret.esm.js",
  "browser": "dist/text-mask-siret.umd.js",
  "author": "Dimitri NICOLAS <dimitri@ooeo.fr>",
  "license": "MIT",
  "homepage": "https://github.com/dimitrinicolas/text-mask-siret",
  "bugs": {
    "url": "https://github.com/dimitrinicolas/text-mask-siret/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimitrinicolas/text-mask-siret.git"
  },
  "keywords": [
    "text-mask",
    "siret",
    "siren"
  ],
  "watch": {
    "build": "./src/**/*.*"
  },
  "scripts": {
    "publish": "clean-publish",
    "build": "rollup -c && uglifyjs dist/text-mask-siret.umd.js -o dist/text-mask-siret.umd.min.js -m",
    "dev": "npm-watch",
    "lint": "eslint src/**/**/*.js",
    "test": "ava"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "ava": "^1.3.1",
    "babel-eslint": "^10.0.1",
    "clean-publish": "^1.1.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "http-server": "^0.11.1",
    "install": "^0.12.2",
    "npm": "^6.9.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "text-mask-core": "^5.1.2",
    "uglify-js": "^3.4.9"
  }
}
